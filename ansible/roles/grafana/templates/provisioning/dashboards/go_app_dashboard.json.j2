{
  "id": null,
  "uid": "go-app-dashboard",
  "title": "Go Application Metrics",
  "tags": ["go", "app", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "graph",
      "title": "HTTP Requests per Second",
      "targets": [
        {
          "expr": "rate(http_requests_total[1m])",
          "legendFormat": "requests/s",
          "interval": ""
        }
      ],
      "lines": true,
      "fill": 1,
      "linewidth": 2,
      "span": 6
    },
    {
      "type": "graph",
      "title": "Request Duration (avg)",
      "targets": [
        {
          "expr": "rate(http_request_duration_seconds_sum[1m]) / rate(http_request_duration_seconds_count[1m])",
          "legendFormat": "avg duration (s)"
        }
      ],
      "lines": true,
      "fill": 1,
      "linewidth": 2,
      "span": 6
    },
    {
      "type": "graph",
      "title": "Memory Usage (bytes)",
      "targets": [
        {
          "expr": "go_memstats_alloc_bytes",
          "legendFormat": "alloc bytes"
        },
        {
          "expr": "go_memstats_heap_alloc_bytes",
          "legendFormat": "heap alloc"
        }
      ],
      "lines": true,
      "fill": 2,
      "linewidth": 2,
      "span": 6
    },
    {
      "type": "graph",
      "title": "Number of Goroutines",
      "targets": [
        {
          "expr": "go_goroutines",
          "legendFormat": "goroutines"
        }
      ],
      "lines": true,
      "fill": 1,
      "linewidth": 2,
      "span": 6
    },
    {
      "type": "singlestat",
      "title": "Uptime (hours)",
      "targets": [
        {
          "expr": "time() - process_start_time_seconds",
          "legendFormat": "uptime"
        }
      ],
      "valueName": "avg",
      "format": "h",
      "span": 3
    }
  ]
}

